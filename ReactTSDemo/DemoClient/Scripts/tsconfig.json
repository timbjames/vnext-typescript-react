{
  "compileOnSave": true,
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noEmitOnError": true,
    "noFallthroughCasesInSwitch": true,
    "removeComments": false,
    "sourceMap": true,
    "target": "es5",
    "outDir": "../wwwroot/js/mylibs",
    "jsx": "react",
    "module": "system"
  },
  "exclude": [
    "node_modules",
    "wwwroot"
  ]
}